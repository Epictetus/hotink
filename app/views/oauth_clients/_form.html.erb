<div class="column_holder">
	<div class="third_column left_column">
		<label for="client_application_name">Name*</label>
	</div>
	<div class="two_thirds_column right_column">
		<%= f.text_field :name, :class=>:text_input %>
	</div>
	<div class="third_column left_column">
		<label for="client_application_url">Main Application URL*</label>
	</div>
	<div class="two_thirds_column right_column">
		<%= f.text_field :url, :class=>:text_input  %>
	</div>
	<div class="third_column left_column">
		<label for="client_application_callback_url">Callback URL*</label>
	</div>
	<div class="two_thirds_column right_column">
		<%= f.text_field :callback_url, :class=>:text_input  %>
	</div>
	<div class="third_column left_column">
		<label for="client_application_support_url">Support URL</label>
	</div>
	<div class="two_thirds_column right_column">
		<%= f.text_field :support_url, :class=>:text_input  %>
	</div>
</div>
